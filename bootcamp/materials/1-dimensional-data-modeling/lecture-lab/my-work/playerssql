SELECT * FROM player_seasons;

create type season_stats as (
    season integer,
    gp integer,
    pts real,
    reb real,
    ast real
    );

create table players(
    player_name text,
    height text,
    college text,
    country text,
    draft_year text,
    draft_round text,
    draft_number text,
    seasons season_stats[],
    current_season integer,
    primary key (player_name, current_season)
)
